{
	"jobs" : {
		"job::{{stage.namespace}}::{{stage.app-name}}" : {
			"packages" : [
				{{#packages}}
				{
					"fqn" : "{{fqn}}"
				}
				{{/packages}}
			],
			"exposed_ports" : [
				0
			],
			"required_ports" : [
				0
			],
			"routes" : [
				{
					"type" : "{{stage.endpoint-type}}",
					"endpoint" : "{{stage.endpoint}}",
					"config" : {
						"/" : [
							{
								"port" : 0
							}
						]
					}
				}
			],
			"state": "started"
		}
	}
}
